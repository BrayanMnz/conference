<!DOCTYPE html SYSTEM "http://www.thymeleaf.org/dtd/xhtml1-strict-thymeleaf-4.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xmlns:th="http://www.thymeleaf.org">
<body>
	<header id="header" th:fragment="menu-index" th:remove="tag">
	 <!-- Header Start -->
        <div class="meeta-header-section meeta-header-2">

            <!-- Header Middle Start -->
            <div class="header-middle header-sticky">
                <div class="container">

                    <div class="header-wrap">
                        <!-- Header Logo Start -->
                        <div class="header-logo">
							<a href="/index.html"><img th:with="rootpath=(${content.rootpath != null} ? ${content.rootpath} : '')" th:src="${rootpath}+'img/jconf-sm-logo-2.png'" alt="JConf Dominicana" title="JConf Dominicana"></a>

                        </div>
                        <!-- Header Logo End -->

                        <!-- Header Navigation Start -->
                        <div class="header-navigation d-none d-lg-block">
                            <ul class="main-menu">
                                <li><a href="/index.html" th:text="#{nav.menu.item.home}">Home</a></li>
                                <li><a th:if="${config.layout_display_tickets}" th:href="${config.layout_display_page_spa}?'/index.html#buy-tickets':'/buy-tickets.html'" href="/buy-tickets.html" th:text="#{nav.menu.item.tickets}">Tickets</a></li>
                                <li><a th:if="${config.layout_display_schedule}" th:href="${config.layout_display_page_spa}?'/index.html#schedule':'/schedule.html'" href="/schedule.html" th:text="#{nav.menu.item.agenda}">Agenda</a></li>
                                <li><a th:if="${config.layout_display_speakers}" th:href="${config.layout_display_page_spa}?'/index.html#speakers':'/speakers.html'" href="/speakers.html" th:text="#{nav.menu.item.speakers}">Speakers</a></li>
                                <li><a href="/presentations.html" th:if="${config.layout_display_presentations_page}" th:text="#{nav.menu.item.presentations}">Presentations</a></li>
                                <li><a href="/code-of-conduct.html" th:text="#{nav.menu.item.codeOfConduct}">Code of conduct</a></li>
                                    
                                <li><a href="/index.html#sponsors" th:text="#{nav.menu.item.sponsors}">Sponsors</a></li>
                                <li><a href="/past-conferences.html" th:text="#{nav.menu.item.pastConferences}">Past Conferences</a></li>
                                <li><a href="/conference-info.html" th:text="#{nav.menu.item.conferenceInfo}">Conference Info</a></li>
                                <li><a href="/organizers.html" th:if="${config.layout_display_organizers_page}"  th:text="#{nav.menu.item.organizers}">Organizers</a></li>
                            </ul>
                        </div>
                        <!-- Header Navigation End -->

                        <!-- Header Meta Start -->
                        <div class="header-meta" th:if="${config.layout_display_buytickets_menu_button}">

                            <div class="header-btn d-none d-md-block">
                                <a th:href="${config.layout_buytickets_url}" href="price.html" class="btn btn-primary">Buy Ticket Now</a>
                            </div>

                        </div>
                        <!-- Header Meta End -->

                    </div>

                </div>
            </div>
            <!-- Header Middle End -->

        </div>
        <!-- Header End -->

	</header><!-- #header -->
